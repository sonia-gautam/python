while True:
  num = input("Enter a number: ")
  if num == "done" :
      break
  try :
    p = int(num)
  except Exception :
    print("Invalid Input")
    continue
  largest = 0
  print(largest)
  for value in p :
      print(p)
      if value > largest :
        largest = value
      else:
        continue
      print("Maximum", largest)
